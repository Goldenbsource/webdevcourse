/* global css */
*{
    font-size: 30px;
    font-wieght: 500px;
}
/* External CCS */
h3{
    color: blue;
}

/* element selector */
h4{
    color: palevioleterd;
}

/* class selector */
.myclass{
    color: darkorange;
}

/* id selector */
#myid{
    color: mediumpurple;
}

/* group selector */
h5,p{
    color: maroon;
}